# Production Environment Variables
# Copy this file to .env and update with your production values

# Server Configuration
PORT=5000
NODE_ENV=production

# Database Configuration - UPDATE WITH YOUR MONGODB URI
MONGO_URI=mongodb+srv://your_username:your_password@your_cluster.mongodb.net/tenant_manager?retryWrites=true&w=majority&appName=tenantManager

# Admin User - UPDATE WITH SECURE CREDENTIALS
ADMIN_EMAIL=admin@yourdomain.com
ADMIN_PASSWORD=YourSecureAdminPassword123!

# JWT Secret - GENERATE A STRONG SECRET KEY
JWT_SECRET=your_super_secure_jwt_secret_key_here_make_it_very_long_and_random_for_production

# CORS Configuration (if needed)
CORS_ORIGIN=https://your-frontend-domain.com
